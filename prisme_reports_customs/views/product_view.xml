<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_reports_customs" model="ir.ui.view">
        <field name="name">product.template.form.reports_customs</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='general_information']//group[@name='group_general']" position="inside">
                <field name="country_origin"/>
            </xpath>
        </field>
    </record>
</odoo>
